{
  "name": "stencila-sibyl",
  "version": "0.26.0",
  "description": "Stencila in containers",
  "scripts": {
    "build": "NODE_ENV=production bankai build client/index.js --verbose",
    "docs-build": "minidocs docs/ --assets docs/assets --css docs/styles.css --logo docs/logo.svg --contents docs/contents.json ---pushstate --output build/docs/",
    "docs-publish": "surge --domain sibyl.surge.sh build/docs/",
    "test": "standard && npm run test-deps && node tests",
    "test-deps": "dependency-check . && dependency-check --extra . --entry bin/index.js --no-dev"
  },
  "bin": {
    "sibyl": "./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stencila/sibyl.git"
  },
  "author": "https://github.com/stencila/sibyl/graphs/contributors",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stencila/sibyl/issues"
  },
  "homepage": "https://github.com/stencila/sibyl#readme",
  "dependencies": {
    "async-collection": "^1.0.1",
    "copy-dir": "^0.3.0",
    "dockerode": "^2.5.0",
    "explain-error": "^1.0.4",
    "fixed-size-chunk-hashing": "^1.0.0",
    "gunzip-maybe": "^1.4.1",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "pump": "^1.0.2",
    "request": "^2.81.0",
    "send": "^0.15.2",
    "sha1": "^1.1.1",
    "shallowequal": "^1.0.1",
    "sheetify": "^6.1.0",
    "slug": "^0.9.1",
    "stencila": "0.27-preview",
    "tachyons": "^4.7.4",
    "tar-filter-stream": "^1.0.0",
    "tar-fs": "^1.15.3",
    "uuid": "^3.1.0",
    "xhr": "^2.4.0",
    "tar-fs": "^1.15.3"
  },
  "devDependencies": {
    "dependency-check": "^2.6.1",
    "istanbul": "^0.4.5",
    "minidocs": "^4.2.3",
    "nodemon": "^1.11.0",
    "split2": "^2.1.1",
    "spok": "^0.8.1",
    "standard": "*",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  }
}
