{
  "name": "stencila-sibyl",
  "version": "0.26.0",
  "description": "Stencila in containers",
  "scripts": {
    "build": "mkdir -p dist && pkg bin/index.js --out-path dist -t node6-linux-x64,node6-macos-x64,node6-win-x64,",
    "docs-build": "minidocs docs/ --assets docs/assets --css docs/styles.css --logo docs/logo.svg --contents docs/contents.json ---pushstate --output build/docs/",
    "docs-publish": "surge --domain sibyl.surge.sh build/docs/",
    "lint": "standard",
    "test": "standard && npm run test-deps && node tests | tap-spec",
    "test-deps": "dependency-check . && dependency-check --extra . --entry bin/index.js --no-dev",
    "cover": "istanbul cover tests | tap-spec"
  },
  "bin": {
    "sibyl": "./bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stencila/sibyl.git"
  },
  "author": "https://github.com/stencila/sibyl/graphs/contributors",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/stencila/sibyl/issues"
  },
  "homepage": "https://github.com/stencila/sibyl#readme",
  "dependencies": {
    "copy-dir": "^0.3.0",
    "dockerode": "^2.5.0",
    "explain-error": "^1.0.4",
    "gunzip-maybe": "^1.4.1",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "opn": "^5.1.0",
    "pump": "^1.0.2",
    "random-port": "^0.1.0",
    "request": "^2.81.0",
    "slug": "^0.9.1",
    "strip-dirs": "^2.0.0",
    "tar-fs": "^1.15.3",
    "unzip-stream": "^0.1.2"
  },
  "devDependencies": {
    "debug": "^2.6.8",
    "dependency-check": "^2.6.1",
    "istanbul": "^0.4.5",
    "minidocs": "^4.2.3",
    "nodemon": "^1.11.0",
    "pkg": "^4.1.2",
    "split2": "^2.1.1",
    "spok": "^0.8.1",
    "standard": "*",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "tmp": "0.0.31"
  }
}
